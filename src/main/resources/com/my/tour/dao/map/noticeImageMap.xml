<?xml version='1.0' encoding='utf-8' ?>
<!DOCTYPE mapper PUBLIC './/mybatis.org//DTD Mapper 3.0//EN'
	'http://mybatis.org/dtd/mybatis-3-mapper.dtd'>
<mapper namespace='com.my.tour.dao.map.NoticeImageMap'>
	<select id='selectNoticeImages' resultType='noticeImage'>
		select *
		from notice_images
		order by noticeImage_id desc
	</select>
	
	<insert id='insertNoticeImages'>
		insert into notice_images
		values (#{noticeImageId}, #{noticeImageName}, #{noticeNum})
	</insert>
</mapper>